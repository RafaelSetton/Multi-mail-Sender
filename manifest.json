{
    "manifest_version": 2,
  
    "name": "Multi-mail Sender",
    "description": "Send multiple e-mails altering some variables",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["content.js"]
        }
    ]
}